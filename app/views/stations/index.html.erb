<p id="notice"><%= notice %></p>

<h1>Listing Stations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Capacity</th>
      <th>Launch Date</th>
      <th>Link</th>
    </tr>
    <% @stations.each do |station| %>
      <tr>
        <td><%= station.id %></td>
        <td><%= station.name %></td>
        <td><%= station.latitude %></td>
        <td><%= station.longitude %></td>
        <td><%= station.dpcapacity %></td>
        <td><%= station.online_date %></td>
        <td><%= link_to 'Show', station %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

